@font-face {
  font-family 'Lato'
  src url('../fonts/LatoLatin-Bold.woff2') format('woff2'), url('../fonts/LatoLatin-Bold.woff') format('woff'), url('../fonts/LatoLatin-Bold.ttf') format('truetype')
  font-style normal
  font-weight bold
  text-rendering optimizeLegibility
}

